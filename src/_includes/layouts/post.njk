---
layout: layouts/base.njk
---
<article class="post">
  <header class="post-header">
    <h1>{{ title }}</h1>
    <time datetime="{{ date | isoDate }}">{{ date | dateFormat }}</time>
  </header>
  {{ content | safe }}
  <nav class="back-link">
    <a href="/journal/">&larr; Journal</a>
  </nav>
</article>
